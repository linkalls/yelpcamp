<% layout("/layouts/boilerplate") %>

<form action="/campgrounds" method="post">
  <div>
    <label for="title">タイトル</label>
    <input type="text" name="campground[title]" id="title" placeholder="タイトル" />
    <!--  name="campground[title]"はcampgroundの中のtitle -->
  </div>
  <div>
    <label for="location">場所</label>
    <input type="text" name="campground[location]" id="location" placeholder="場所" />
  </div>
  <button>登録する</button>
</form>
